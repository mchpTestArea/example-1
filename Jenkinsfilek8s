pipeline {
    agent {
        kubernetes {
            label 'relative'
			defaultContainer 'xc8-mplabx'
            yamlFile 'cloudprovider.yml'
        }
    }
	 stages {
        stage('Checkout') {
            steps {
				checkout scm
            }
        }
		stage('base') {
			steps {
				 build job: "User Repositories/Dinesh Baria/base/master", 
							parameters: [
								string(name: 'repoName', value: "relative",
								string(name: 'branchName', value: "{env.BRANCH_NAME}")
							],
							propagate:true, wait: true
			}
		}
	}
}	
def execute(String cmd) {
	if(isUnix()) {
		sh cmd
	} else {
		bat cmd
	}
}