pipeline {
    agent {
        kubernetes {
            label 'relative'
			defaultContainer 'xc8-mplabx'
            yamlFile 'cloudprovider.yml'
        }
    }
	 stages {
        stage('Checkout') {
            steps {
				checkout scm
            }
        }
		stage('base') {
			steps {
				//def downstreamJob = build job: "User Repositories/Dinesh Baria/base/master", 
				//			parameters: [
				//				string(name: 'REPO_NAME', value: "relative"),
				//				string(name: 'BRANCH_NAME', value: "${env.BRANCH_NAME}")
				//			],
				//			propagate:true, wait: true
				def downstreamJob = build('User Repositories/Dinesh Baria/base/master')
				echo "${downstreamJob}"
			}
		}
	}
}	
def execute(String cmd) {
	if(isUnix()) {
		sh cmd
	} else {
		bat cmd
	}
}